<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
                             http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.example.servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>TeacherDashboardServlet</servlet-name>
        <servlet-class>com.example.servlet.TeacherDashboardServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TeacherDashboardServlet</servlet-name>
        <url-pattern>/teacher_dashboard</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>PrintRequestServlet</servlet-name>
        <servlet-class>com.example.servlet.PrintRequestServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PrintRequestServlet</servlet-name>
        <url-pattern>/request_print</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AgentDashboardServlet</servlet-name>
        <servlet-class>com.example.servlet.AgentDashboardServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AgentDashboardServlet</servlet-name>
        <url-pattern>/agent_dashboard</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AdminDashboardServlet</servlet-name>
        <servlet-class>com.example.servlet.AdminDashboardServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AdminDashboardServlet</servlet-name>
        <url-pattern>/admin_dashboard</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UserManagementServlet</servlet-name>
        <servlet-class>com.example.servlet.UserManagementServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserManagementServlet</servlet-name>
        <url-pattern>/manage_users</url-pattern>
    </servlet-mapping>
    <servlet>
    <servlet-name>LogoutServlet</servlet-name>
    <servlet-class>com.example.servlet.LogoutServlet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/logout</url-pattern>
</servlet-mapping>

    <session-config>
        <session-timeout>30</session-timeout>
    </session-config>
</web-app>
